		"@testing-library/react": "^11.2.2",
		"@testing-library/user-event": "^12.5.0",
		"axios": "^0.21.1",
		"chartist": "0.10.1",
		"classnames": "2.2.6",
		"downloadjs": "^1.4.7",
		"file-saver": "^2.0.5",
		"generate-password": "^1.6.0",
		"history": "^4.10.1",
		"jwt-decode": "^3.1.2",
		"lodash": "^4.17.20",
		"perfect-scrollbar": "1.5.0",
		"primeicons": "^4.1.0",
		"primereact": "^6.2.1",
		"prop-types": "15.7.2",
