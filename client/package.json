		"react-data-grid-addons": "^7.0.0-alpha.24",
		"react-datepicker": "^3.5.0",
		"react-datetime-picker": "^3.1.0",
		"react-dom": "^17.0.1",
		"react-google-maps": "9.4.5",
		"react-hook-form": "^6.12.2",
